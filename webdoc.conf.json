{
  "opts": {
    "destination": "guides",
    "import": ["https://api.pixijs.io/pixijs.api.json"]
  },
  "template": {
    "alias": {
      "explorer": "./template/explorer.tmpl",
      "header": "./template/header.tmpl",
      "bottom-banner": "./template/bottom-banner.tmpl"
    },
    "applicationName": "<a href=\"https://www.pixijs.com\" class=\"pixi-logo\"><img src=\"https://www.pixijs.com/img/blank.gif\" alt=\"PixiJS Logo\"></a>",
    "appBar": {
      "items": {
        "tutorials": {
          "name": "Guides"
        },
        "download": {
          "name": "GitHub",
          "uri": "https://github.com/pixijs/pixijs"
        },
        "examples": {
          "name": "Examples",
          "uri": "https://pixijs.io/examples"
        },
        "gallery": {
          "name": "API Docs",
          "uri": "https://pixijs.io/docs"
        },
        "support": {
          "name": "Support Us",
          "uri": "https://opencollective.com/pixijs"
        }
      }
    },
    "variant": "plain",
    "routes": {
      "tutorials": ""
    },
    "siteDomain": "https://pixijs.io/",
    "siteRoot": "guides",
    "stylesheets": [
      "./static/styles/index.css",
      "./static/styles/header.css",
      "./static/styles/md.css",
      "./static/styles/layout.css",
      "./static/styles/explorer.css",
      "./static/styles/nav.css",
      "./static/styles/bottom-banner.css"
    ],
    "title": "PixiJS",
    "meta": {
      "og:title": "PixiJS Guides",
      "og:description": "The holy guide for PixiJS developers",
      "og:image": "https://camo.githubusercontent.com/dd5f0c8ce84c09599ebd30c7a48076b3ca4ef7d1f36aed96c64952d3432942a1/68747470733a2f2f706978696a732e646f776e6c6f61642f706978696a732d62616e6e65722d6e6f2d76657273696f6e2e706e67",
      "og:site": "https://pixijs.io/guides/",
      "viewport": "width=device-width, initial-scale=1"
    }
  }
}
